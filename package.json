{
  "name": "jardinsecret-back",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "8.16.x",
    "npm": "5.6 x"
  },
  "scripts": {
    "build": "babel -d ./dist --ignore '**/node_modules/**/*' --source-maps true --extensions '.js,.ts' .",
    "build:all": "npm run build && (cd dist; yarn)",
    "check-circular:all": "npm run build:all && (cd dist; npm run check-circular)",
    "check-circular": "madge --circular ./",
    "check-db": "NODE_ENV=test ./run-script check.db",
    "init-db": "NODE_ENV=test ./run-script db.init.migrations",
    "lint": "eslint --fix --cache .",
    "migrate": "sequelize db:migrate",
    "migrate-db": "./run-script db.migrate",
    "reinstall": "yarn cache clean && rm -rf node_modules && yarn",
    "serve": "nodemon . | pino-pretty -t -i hostname",
    "spec": "NODE_ENV=test mocha --bail --require babel-register.js",
    "start": "node .",
    "test": "NODE_ENV=test LOG_LEVEL=debug mocha --opts mocha.opts --bail | pino-pretty -t -i hostname",
    "yarn": "yarn install --frozen-lockfile"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@expo/spawn-async": "^1.3.0",
    "async-lock": "^0.3.9",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.5.0",
    "chai-lint": "^0.1.1",
    "chai-subset": "^1.6.0",
    "cls-hooked": "^4.2.2",
    "cls-proxify": "^1.0.1",
    "compression": "^1.6.2",
