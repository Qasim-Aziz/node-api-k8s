{
  "name": "jardinsecret-back",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "12.19.x",
    "npm": "6.14.x"
  },
  "scripts": {
    "build": "tsc",
    "build:all": "npm run build && (cd dist; yarn)",
    "check-circular:all": "npm run build:all && (cd dist; npm run check-circular)",
    "check-circular": "madge --circular ./",
    "check-db": "",
    "init-db": "NODE_ENV=test ./run-script.js db.init",
    "lint": "eslint --quiet --fix --cache .",
    "migrate": "FORCE_UNIQUE_TRANSACTION=true ./run-script.js db.migrate",
    "init-data": "NODE_ENV=test FORCE_UNIQUE_TRANSACTION=true ./run-script.js init-data | pino-pretty -i hostname",
    "migration:create": "migrate create --chdir $(pwd)/src --template-file $(pwd)/src/migrations/lib/template.js --generator $(pwd)/src/migrations/lib/template-generator.js",
    "reinstall": "yarn cache clean && rm -rf node_modules && yarn",
    "spec": "NODE_ENV=test mocha --bail --require babel-register.js",
    "serve": "nodemon | pino-pretty -i hostname",
    "test": "jest --bail --runInBand --detectOpenHandles",
    "yarn": "yarn install --frozen-lockfile"
  },
  "dependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/joi": "^14.3.4",
    "@types/node": "^14.14.6",
    "@types/winston": "^2.4.4",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cls-proxify": "^1.0.1",
    "compression": "^1.7.4",
    "convict": "^6.0.0",
    "convict-format-with-validator": "^6.0.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "http-status": "^1.4.2",
    "hyperid": "^2.0.5",
    "jest": "^26.4.2",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "migrate": "^1.7.0",
    "mockdate": "^3.0.2",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "seedrandom": "^3.0.5",
    "sequelize": "^6.3.5",
    "set-cookie-parser": "^2.4.6",
    "sinon": "^9.2.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "yargs": "^16.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0"
  }
}
